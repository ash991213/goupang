        "shipment": "pnpm --filter ./apps/shipment",
        "user": "pnpm --filter ./apps/user",
        "auth": "pnpm --filter ./apps/auth",
        "module": "pnpm --filter ./libs/module",
        "util": "pnpm --filter ./libs/util",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\""
    },
    "dependencies": {
