{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/goupang/src",
    "compilerOptions": {
        "deleteOutDir": false,
        "tsConfigPath": "apps/goupang"
    },
    "monorepo": true,
    "root": "apps/goupang",
    "projects": {
        "user": {
            "type": "application",
            "root": "apps/user",
            "entryFile": "main",
            "sourceRoot": "apps/user/src",
            "compilerOptions": {
                "tsConfigPath": "apps/user/tsconfig.app.json"
            }
        },
        "module": {
            "type": "library",
            "root": "libs/module",
            "entryFile": "index",
            "sourceRoot": "libs/module/src",
            "compilerOptions": {
                "tsConfigPath": "libs/module/tsconfig.lib.json"
            }
        },
        "product": {
            "type": "application",
            "root": "apps/product",
            "entryFile": "main",
            "sourceRoot": "apps/product/src",
            "compilerOptions": {
                "tsConfigPath": "apps/product/tsconfig.app.json"
            }
        },
        "host": {
            "type": "application",
            "root": "apps/host",
            "entryFile": "main",
            "sourceRoot": "apps/host/src",
            "compilerOptions": {
                "tsConfigPath": "apps/host/tsconfig.app.json"
            }
        },
        "order": {
            "type": "application",
            "root": "apps/order",
            "entryFile": "main",
            "sourceRoot": "apps/order/src",
            "compilerOptions": {
                "tsConfigPath": "apps/order/tsconfig.app.json"
            }
        },
        "util": {
            "type": "library",
            "root": "libs/util",
            "entryFile": "index",
            "sourceRoot": "libs/util/src",
            "compilerOptions": {
                "tsConfigPath": "libs/util/tsconfig.lib.json"
            }
        },
        "auth": {
            "type": "application",
            "root": "apps/auth",
            "entryFile": "main",
            "sourceRoot": "apps/auth/src",
            "compilerOptions": {
                "tsConfigPath": "apps/auth/tsconfig.app.json"
            }
        },
        "shipment": {
            "type": "application",
            "root": "apps/shipment",
            "entryFile": "main",
            "sourceRoot": "apps/shipment/src",
            "compilerOptions": {
                "tsConfigPath": "apps/shipment/tsconfig.app.json"
            }
        },
        "noti": {
            "type": "application",
            "root": "apps/noti",
            "entryFile": "main",
            "sourceRoot": "apps/noti/src",
            "compilerOptions": {
                "tsConfigPath": "apps/noti/tsconfig.app.json"
            }
        },
        "payment": {
            "type": "application",
            "root": "apps/payment",
            "entryFile": "main",
            "sourceRoot": "apps/payment/src",
            "compilerOptions": {
                "tsConfigPath": "apps/payment/tsconfig.app.json"
            }
        }
    }
}
